<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="20dp"
    android:layout_margin="10dp"
    android:orientation="vertical">

    <TextView
        android:id="@+id/bevattningar_info_ui_plant_text1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:background="#B0C4DE"
        android:textColor="#000000"/>

    <TextView
        android:id="@+id/bevattningar_info_ui_waterAmount_text1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:background="#B0C4DE"
        android:textColor="#000000"/>

    <TextView
        android:id="@+id/bevattningar_info_ui_timeHour_text1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:background="#B0C4DE"
        android:textColor="#000000"/>

    <TextView
        android:id="@+id/bevattningar_info_ui_timeMin_text1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:background="#B0C4DE"
        android:textColor="#000000"/>

    <TextView
        android:id="@+id/bevattningar_info_ui_orderNumber_text1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:background="#B0C4DE"
        android:textColor="#000000"/>

    <TextView
        android:id="@+id/bevattningar_info_ui_accountComperer_text1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:background="#B0C4DE"
        android:textColor="#000000"/>
</LinearLayout>